<canvas id="wordcloud_<%= title %>" width="320" height="220">
  <script type="text/javascript">
    WordCloud(
      document.getElementById('wordcloud_<%= title %>'),
      {
        list: <%= raw wordcloud %>,
        drawOutOfBound: true,
        color: 'black',
        weightFactor: 50
      }
    );
  </script>
</canvas>
